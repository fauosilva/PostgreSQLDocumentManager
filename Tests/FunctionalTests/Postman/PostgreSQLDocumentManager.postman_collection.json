	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"var bearerToken = pm.collectionVariables.get('bearerToken');",
					"",
					"if(!bearerToken)",
					"{",
					"   generateJwtToken();",
					"}",
					"else",
					"{    ",
					"    let jwt = bearerToken;",
					"    let [jwtHeader, jwtPayload, jwtSignature] = jwt.split('.')",
					"    let jwtPayloadJsonString = atob(jwtPayload);",
					"    //console.log(jwtPayloadJsonString);",
					"",
					"    if(jwtPayloadJsonString){",
					"        var jsonPayload = JSON.parse(jwtPayloadJsonString);",
					"        var currentDate = Math.floor(new Date().getTime()/1000);",
					"        ",
					"        var exp = jsonPayload.exp;        ",
					"        if( exp < currentDate){            ",
					"            console.log(\"Token expired.\");",
					"            generateJwtToken();",
					"        }",
					"    }",
					"}",
					"",
					"function generateJwtToken(){",
					"    console.log(\"Generating a new jwt token\");",
					"    var baseUrl = pm.collectionVariables.get(\"baseUrl\");",
					"",
					"    const postRequest = {",
					"        url: baseUrl + '/api/v1/Authentication/Login',",
					"        method: 'POST',",
					"        timeout: 0,",
					"        header: {",
					"            \"Content-Type\": \"application/json\"",
					"        },",
					"        body: {",
					"            mode: 'raw',",
					"            raw: JSON.stringify({ 'username': 'adminuser', 'password': 'Adminpassword' })",
					"        }",
					"    };",
					"    pm.sendRequest(postRequest, function (err, res) {",
					"        var responseJson = res.json();",
					"        console.log(responseJson);",
					"        pm.collectionVariables.set('bearerToken', responseJson['jwtToken']);",
					"    });",
					"}"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
